type Profile {
  id: ID! @unique
  account_id: ID @unique
  userName: String @unique
  firstName: String
  lastName: String
  dateOfBirth: String
  storiesPublished: Int @default(value: 0)
  storiesReviewed: Int @default(value: 0)
  flaggedStories: Int @default(value: 0)
  occupation: String @default(value: "N/A")
  employer: Organization
  storiesLiked: [Likes!]!
  communitiesIds: [ID!]!
}

type Likes {
  id: ID! @unique
  guid: String @unique
}

type Organization {
  id: ID! @unique
  orgName: String!
  address: String!
  country: String!
  orgType: String!
}
